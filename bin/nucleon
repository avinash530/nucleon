#!/usr/bin/env ruby

require 'nucleon'

# Set version info

module Nucleon; module Util; module CLI
class Parser
  def version
    version = Nucleon.VERSION
    "Nucleon: #{version}  (Apache License, Version 2.0)  --  https://github.com/coralnexus/nucleon/tree/v#{version}"  
  end
end
end; end; end

# Execute

exit(Nucleon.executable(ARGV, :nucleon))
